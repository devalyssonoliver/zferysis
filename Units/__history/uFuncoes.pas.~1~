unit uFuncoes;

interface
 uses Vcl.Forms, Vcl.Controls, Vcl.StdCtrls, Vcl.ExtCtrls;

procedure DesabilitarCampos(var Campos: array of TLabeledEdit);

implementation

procedure DesabilitarCampos(var Campos: array of TLabeledEdit);

var
  i: Integer;
begin
  for i := 0 to High(Campos) do
  begin
    Campos[i].Enabled := False;
  end;
end;

end.
